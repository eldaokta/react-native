<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_4.xsd"
         version="2.4">

  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.enigma.Hello</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApaKabar</servlet-name>
    <servlet-class>com.enigma.ApaKabar</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RobotServlet</servlet-name>
    <servlet-class>com.enigma.RobotServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StudentServlet</servlet-name>
    <servlet-class>com.enigma.StudentServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello.php</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ApaKabar</servlet-name>
    <url-pattern>/apakabar.php</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RobotServlet</servlet-name>
    <url-pattern>/robotservlet.php</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StudentServlet</servlet-name>
    <url-pattern>/student.php</url-pattern>
  </servlet-mapping>
</web-app>
